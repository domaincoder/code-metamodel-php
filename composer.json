{
    "name": "domaincoder/code-metamodel-php",
    "type": "library",
    "description": "CodeMetamodel-PHP provides metamodel classes of PHP code and a parser which parses PHP codes and build metamodel objects.",
    "keywords": ["metamodel"],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "GOTO Hidenori",
            "email": "hidenorigoto@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "DomainCoder\\Metamodel\\Code\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-develop": "0.1.x-dev"
        }
    },
    "require": {
        "php": ">=5.5.0",
        "phpmentors/domain-kata": "~1.3",
        "nikic/php-parser": "~1.1",
        "herrera-io/annotations": "~1.0",
        "doctrine/annotations": "~1.3",
        "lstrojny/functional-php": "dev-master",
        "symfony/console": "~2.6",
        "symfony/finder": "~2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3",
        "phake/phake": "~2.0"
    },
    "config": {
        "bin-dir": "bin"
    }
}
